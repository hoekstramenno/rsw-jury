{
    "name": "scoutinghaarlem/rsw-scoreboard",
    "type": "project",
    "description": "Scoreboard application for RSW Haarlem",
    "keywords": [
        "scouting",
        "haarlem",
        "rsw"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.19",
        "ext-json": "*",
        "fideloper/proxy": "~4.0",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "knplabs/knp-snappy": "^1.1",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "maatwebsite/excel": "^3.1",
        "spatie/data-transfer-object": "^1.13"
    },
    "require-dev": {
        "czim/phpunit-printer": "^0.9.1",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "platform": {
            "php": "7.2.19"
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
